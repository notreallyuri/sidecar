{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "downloader-capability",
  "description": "Allow execution of the downloader sidecar",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/downloader-sidecar",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
